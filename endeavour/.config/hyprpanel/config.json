{
  "scalingPriority": "both",
  "notifications.position": "bottom right",
  "theme.bar.location": "bottom",
  "bar.workspaces.showWsIcons": true,
  "bar.workspaces.showApplicationIcons": false,
  "theme.bar.buttons.modules.ram.enableBorder": false,
  "bar.volume.scrollDown": "hyprpanel vol -2",
  "bar.volume.scrollUp": "hyprpanel vol +2",
  "bar.bluetooth.label": true,
  "theme.bar.floating": false,
  "bar.autoHide": "fullscreen",
  "bar.layouts": {
    "*": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "network",
        "systray",
        "clock",
        "notifications"
      ]
    }
  },
  "theme.bar.scaling": 75,
  "theme.notification.scaling": 75,
  "theme.osd.scaling": 75,
  "theme.bar.menus.menu.dashboard.scaling": 75,
  "theme.bar.menus.menu.dashboard.confirmation_scaling": 75,
  "theme.bar.menus.menu.media.scaling": 75,
  "theme.bar.menus.menu.volume.scaling": 75,
  "theme.bar.menus.menu.network.scaling": 75,
  "theme.bar.menus.menu.bluetooth.scaling": 75,
  "theme.bar.menus.menu.battery.scaling": 75,
  "theme.bar.menus.menu.clock.scaling": 75,
  "theme.bar.menus.menu.notifications.scaling": 75,
  "theme.bar.menus.menu.power.scaling": 70,
  "theme.tooltip.scaling": 75,
  "menus.clock.weather.enabled": false,
  "menus.clock.weather.location": "Jyväskylä",
  "menus.clock.weather.unit": "metric",
  "menus.dashboard.powermenu.logout": "uwsm stop",
  "menus.dashboard.shortcuts.enabled": true,
  "theme.bar.transparent": false,
  "theme.bar.buttons.style": "default",
  "theme.bar.opacity": 50,
  "terminal": "foot",
  "menus.dashboard.directories.left.directory1.command": "foot bash -c \"xdg-open $HOME/Downloads/\"",
  "menus.dashboard.directories.left.directory2.command": "foot bash -c \"xdg-open $HOME/Videos/\"",
  "menus.dashboard.directories.left.directory3.command": "foot bash -c \"xdg-open $HOME/Projects/\"",
  "menus.dashboard.directories.right.directory1.command": "foot bash -c \"xdg-open $HOME/Documents/\"",
  "menus.dashboard.directories.right.directory2.command": "foot bash -c \"xdg-open $HOME/Pictures/\"",
  "menus.dashboard.directories.right.directory3.command": "foot bash -c \"xdg-open $HOME/\""
}